<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <listener>
        <listener-class>comet.Constants</listener-class>
     </listener>
     
  <servlet>
  	<servlet-name>CometServlet</servlet-name>
  	<display-name>CometServlet</display-name>
  	<description></description>
  	<servlet-class>comet.CometServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetMsg</servlet-name>
  	<display-name>GetMsg</display-name>
  	<description></description>
  	<servlet-class>comet.GetMsg</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CometServlet</servlet-name>
  	<url-pattern>/CometServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetMsg</servlet-name>
  	<url-pattern>/GetMsg</url-pattern>
  </servlet-mapping>
</web-app>
